fresh=float(input("enter the number of fresh loaves"))
old=float(input("enter the number of day old loaves"))
amt_of_fresh=fresh*185.00
amt_of_old=old*185*.6
print("regular_price=185.00")
print("amount of fresh leaves",amt_of_fresh)
print("amount of old leaves",amt_of_old)
print("Total Amount to be paid",amt_of_fresh+
amt_of_old)
